name: subfloat subfig
<figure class="subfloat" id="{{ obj.id }}">
{{ obj }}
{% if obj.title -%}
<figcaption>
<span class="subref">{{ obj.subref }}</span>
<span class="subcaption">{{ obj.title }}</span>
</figcaption>
{% endif -%}
</figure>

name: subref subref*
<a href="{{ obj.idref.label.url }}">{{ obj.idref.label.subref }}</a>

